{
    "name": "electrik/electrik",
    "description": "Starter kit for Laravel based SaaS projects",
    "license": "MIT",
    "authors": [
        {
            "name": "neerajsohal",
            "email": "hello@neerajkumar.name"
        }
    ],
	"require": {
        "php": "^8.1",
        "illuminate/support": "^9.31"
    },
	"autoload": {
        "psr-4": {
            "Electrik\\": "src",
            "Electrik\\Database\\Factories\\": "database/factories"
        },
		"files": [
			"src/helpers.php"
		]
    },
    "autoload-dev": {
        "psr-4": {
            "Electrik\\Tests\\": "tests"
        }
    },
	"config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Electrik\\ElectrikServiceProvider"
            ],
            "aliases": {
                "Electrik": "Electrik\\Facades\\Electrik"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "livewire/livewire": "^2.10"
    }
}

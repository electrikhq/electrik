{
    "name": "electrik/electrik",
    "description": "Starter kit for Laravel based SaaS projects",
    "license": "MIT",
    "authors": [
        {
            "name": "neerajsohal",
            "email": "hello@neerajkumar.name"
        }
    ],
	"require": {
        "php": "^8.1|^8.2|^8.3",
        "ext-bcmath": "*",
        "electrik/slate": "dev-development",
        "illuminate/support": "^11.0.8",
        "laravel/cashier": "^15.3",
        "livewire/livewire": "^3.4",
        "mpociot/teamwork": "dev-l11-compatibility as 8.1.0",
        "rappasoft/laravel-livewire-tables": "^3.2",
        "spatie/laravel-permission": "^6.4",
        "usernotnull/tall-toasts": "^2.1",
        "wire-elements/modal": "^2.0"
    },
	"autoload": {
        "psr-4": {
            "Electrik\\": "src"
        },
		"files": [
            "src/Helpers/timezones.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Electrik\\Tests\\": "tests"
        }
    },
	"config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Electrik\\ElectrikServiceProvider"
            ],
            "aliases": {
                "Electrik": "Electrik\\Facades\\Electrik"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/teamwork.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
